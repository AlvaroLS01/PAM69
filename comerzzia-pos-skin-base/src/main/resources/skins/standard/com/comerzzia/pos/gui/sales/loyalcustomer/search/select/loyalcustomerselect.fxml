            </TableView>
          </children>
        </HBox>
        <HBox alignment="CENTER_RIGHT" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0" styleClass="button-panel-box" VBox.vgrow="NEVER">
          <children>
            <Button fx:id="btAccept" contentDisplay="LEFT" mnemonicParsing="false" onAction="#actionAccept" prefHeight="45.0" prefWidth="112.0" styleClass="main-btn" text="%Aceptar" textAlignment="LEFT" />
            <Button fx:id="btCancel" mnemonicParsing="false" onAction="#closeCancel" prefHeight="45.0" prefWidth="112.0" styleClass="btn" text="%Cancelar" />
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@../../../gui/styles/styles.css" />
  </stylesheets>
