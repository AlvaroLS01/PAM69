<AnchorPane id="AnchorPane" prefHeight="423.0" prefWidth="696.0" styleClass="" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" >
  <children>
    <VBox prefHeight="-1.0" prefWidth="-1.0" styleClass="background-card-pane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <HBox prefHeight="-1.0" prefWidth="-1.0" styleClass="titulo-ventana">
          <children>
            <Label fx:id="lbTitle" alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" text="Selección de Fidelizado" textAlignment="CENTER" HBox.hgrow="ALWAYS" />
          </children>
        </HBox>
        <HBox prefHeight="423.0" prefWidth="696.0" styleClass="czz-card" VBox.vgrow="ALWAYS">
          <children>
            <TableView fx:id="tbLoyalCustomers" onMouseClicked="#acceptDocDoubleClick" prefHeight="357.0" prefWidth="691.0" tableMenuButtonVisible="false" HBox.hgrow="ALWAYS">
              <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
              </columnResizePolicy>
              <columns>
                <TableColumn maxWidth="5000.0" minWidth="0.0" prefWidth="142.0" text="%Código" fx:id="tcLyCustomerCode" />
                <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="319.0" text="%Apellidos" fx:id="tcLastName" />
                <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="319.0" text="%Nombre" fx:id="tcName" />
                <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="319.0" text="%Direccion" fx:id="tcAddress" />
                <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="319.0" text="%Documento" fx:id="tcDocument" />
                <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="319.0" text="%Tarjeta" fx:id="tcCard" />
