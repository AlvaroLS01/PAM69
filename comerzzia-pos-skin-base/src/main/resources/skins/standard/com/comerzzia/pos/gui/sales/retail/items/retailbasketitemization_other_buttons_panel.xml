<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ButtonsGroup>
	<ButtonsRow>       
		<Button>
			<text>Selección Manual</text>
            <key>openPickList</key>
            <type>METODO</type>
			<shortcut>F9</shortcut>
			<imagePath>icons/search.png</imagePath>
			<visible>S</visible>
        </Button>
	</ButtonsRow>
	<ButtonsRow>
       	<Button>
			<text>Canjear puntos</text>
            <key>redeemPoints</key>
            <type>METODO</type>
			<shortcut>F10</shortcut>
			<imagePath>icons/customer-sale.png</imagePath>
			<visible>S</visible>
        </Button> 
	</ButtonsRow> 
	<ButtonsRow>
       	<Button>
			<text>Consultar Saldo</text>
            <key>openCheckGiftCardBalance</key>
            <type>METODO</type>
			<shortcut>F11</shortcut>
			<imagePath>icons/gift.png</imagePath>
			<visible>S</visible>
        </Button> 
	</ButtonsRow> 
	<ButtonsRow>
       	<Button>
			<text>Envío a domicilio</text>
            <key>selectService</key>
            <type>METODO</type>
			<shortcut>F11</shortcut>
			<imagePath>icons/customer-sale.png</imagePath>
			<visible>S</visible>
			<params>
                <entry>
	                <key>serviceTypeCode</key>
	                <value>0010</value>
                </entry>
            </params>
        </Button> 
	</ButtonsRow> 
	<ButtonsRow>
       	<Button>
			<text>Online catalog</text>
            <key>openOnlineCatalog</key>
            <type>METODO</type>
			<shortcut>Ctrl + I</shortcut>
			<imagePath>icons/search.png</imagePath>
			<visible>S</visible>
			<params>
                <entry>
	                <key>url</key>
	                <value>http://solfood.des.comerzzia.com/</value>
                </entry>
            </params>
        </Button> 
	</ButtonsRow> 
	<ButtonsRow>
       	<Button>
			<text>Recuperar cesta</text>
            <key>retrieveRemoteBasket</key>
            <type>METODO</type>
			<shortcut>Ctrl + R</shortcut>
			<imagePath>icons/ticket-retrieve.png</imagePath>
			<visible>S</visible>
        </Button> 
	</ButtonsRow> 
</ButtonsGroup>