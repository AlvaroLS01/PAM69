<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-stylesheet ../../../../pos/gui/styles/styles.css?>
<?scenebuilder-stylesheet ../../../../pos/gui/styles/colors.css?>

<AnchorPane id="AnchorPane" maxHeight="400.0" maxWidth="800.0" minHeight="-1.0" prefHeight="400.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
  <children>
    <VBox prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <HBox prefHeight="-1.0" prefWidth="-1.0" styleClass="titulo-ventana" VBox.vgrow="NEVER">
          <children>
            <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="800.0" text="%Estados anteriores del servicio" textFill="BLACK" HBox.hgrow="ALWAYS" />
          </children>
        </HBox>
        <HBox prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
          <children>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
              <children>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                  <children>
                    <TableView fx:id="statesTable" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS">
                      <columns>
                        <TableColumn fx:id="tcPreviousStatus" editable="false" maxWidth="5000.0" minWidth="10.0" prefWidth="190.0" text="%Estado anterior" />
                        <TableColumn fx:id="tcNewStatus" editable="false" maxWidth="5000.0" minWidth="10.0" prefWidth="190.0" text="%Estado nuevo" />
                        <TableColumn fx:id="tcDate" editable="false" maxWidth="5000.0" minWidth="10.0" prefWidth="100.0" text="%Fecha" />
                        <TableColumn fx:id="tcUser" prefWidth="100.0" text="%Usuario" />
                        <TableColumn fx:id="tcRemarks" prefWidth="220.0" text="%Observaciones" />
                      </columns>
                    </TableView>
                  </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="-1.0" prefWidth="-1.0" styleClass="button-panel-box" VBox.vgrow="NEVER">
                  <children>
                    <Button fx:id="btnClose" mnemonicParsing="false" onAction="#closeCancel" prefWidth="101.0" text="%Cerrar">
                                 <HBox.margin>
                                    <Insets right="5.0" />
                                 </HBox.margin></Button>
                  </children>
                </HBox>
              </children>
            </VBox>
          </children>
          <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </padding>
        </HBox>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@../../../../pos/gui/styles/styles.css" />
  </stylesheets>
</AnchorPane>
