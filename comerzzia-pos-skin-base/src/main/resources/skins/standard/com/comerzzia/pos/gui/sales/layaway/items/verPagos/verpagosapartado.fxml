<?xml version="1.0" encoding="UTF-8"?>

<?import com.comerzzia.pos.core.gui.components.numerickeypad.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?scenebuilder-stylesheet ../../../../styles/colors.css?>
<?scenebuilder-stylesheet ../../../../styles/styles.css?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" styleClass="" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" >
  <!-- TODO Add Nodes -->
  <children>
    <VBox prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <HBox minHeight="50.0" prefHeight="50.0" prefWidth="-1.0" styleClass="titulo-ventana">
          <children>
            <Label alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" text="%Pagos del apartado" />
          </children>
        </HBox>
        <HBox prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
          <children>
            <VBox prefHeight="-1.0" prefWidth="-1.0" styleClass="background-card-pane" HBox.hgrow="ALWAYS">
              <children>
                <HBox prefHeight="-1.0" prefWidth="-1.0" styleClass="card" VBox.vgrow="ALWAYS">
                  <children>
                    <TableView fx:id="tbPagos" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS">
                      <columns>
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="149.0" text="%Fecha" fx:id="tcFecha" />
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="268.0" text="%Medio de pago" fx:id="tcMedioPago" />
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="133.0" text="%Importe" fx:id="tcImporte" />
                      </columns>
                    </TableView>
                  </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0">
                  <children>
                    <Button mnemonicParsing="false" onAction="#accionAceptar" prefWidth="170.0" styleClass="main-btn" text="%Imprimir pago" />
                    <Button mnemonicParsing="false" onAction="#closeCancel" prefWidth="120.0" styleClass="btn" text="%Salir" />
                  </children>
                  <padding>
                    <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                  </padding>
                  <VBox.margin>
                    <Insets />
                  </VBox.margin>
                </HBox>
              </children>
            </VBox>
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@../../../../styles/styles.css" />
  </stylesheets>
</AnchorPane>
