                            <Button mnemonicParsing="false" onAction="#actionAccept" prefHeight="42.0" prefWidth="115.0" styleClass="main-btn" text="%Aceptar" />
                            <Button mnemonicParsing="false" onAction="#closeCancel" prefHeight="42.0" prefWidth="115.0" styleClass="btn" text="%Cancelar" />
                          </children>
                          <HBox.margin>
                            <Insets bottom="5.0" right="5.0" />
                          </HBox.margin>
