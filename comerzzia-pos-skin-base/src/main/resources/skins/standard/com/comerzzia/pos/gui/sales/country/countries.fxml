<AnchorPane id="AnchorPane" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" styleClass="" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <HBox minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <VBox prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS">
          <children>
            <AnchorPane prefHeight="-1.0" prefWidth="-1.0" styleClass="titulo-ventana" VBox.vgrow="NEVER">
              <children>
                <Label alignment="CENTER" text="%Buscar países" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
              </children>
            </AnchorPane>
            <VBox prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
              <children>
                <HBox prefHeight="122.0" prefWidth="1138.0" VBox.margin="$x1">
                  <children>
                    <FlowPane columnHalignment="CENTER" orientation="VERTICAL" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS">
                      <children>
                        <FlowPane alignment="CENTER_LEFT" hgap="5.0" prefHeight="42.0" prefWidth="526.0">
                          <children>
                            <Label alignment="CENTER_RIGHT" prefWidth="146.0" text="%Cod. País" />
