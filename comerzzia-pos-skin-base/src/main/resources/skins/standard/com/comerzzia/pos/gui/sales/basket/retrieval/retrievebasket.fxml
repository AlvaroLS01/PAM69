<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-stylesheet ../../../styles/colors.css?>
<?scenebuilder-stylesheet ../../../styles/styles.css?>

<AnchorPane id="AnchorPane" maxWidth="-1.0" prefHeight="768.0" prefWidth="1024.0" styleClass="background-card-pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <VBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <HBox id="" alignment="CENTER" minHeight="-1.0" prefHeight="-1.0" spacing="0.0" styleClass="titulo-ventana" VBox.vgrow="SOMETIMES">
          <children>
            <Label id="lbNombreAccion" alignment="CENTER" maxHeight="-1.0" minHeight="-1.0" prefHeight="-1.0" prefWidth="-1.0" text="%Recuperar ticket" HBox.hgrow="ALWAYS" />
          </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" styleClass="czz-card" VBox.margin="$x1" VBox.vgrow="ALWAYS">
          <children>
            <TableView fx:id="tbBaskets" onKeyReleased="#acceptLineEnter" onMouseClicked="#actionTableAcceptDocument" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS">
              <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
              </columnResizePolicy>
              <columns>
                <TableColumn id="tcDate" fx:id="tcDate" editable="false" resizable="true" text="%Fecha" />
                <TableColumn id="tcAlias" fx:id="tcAlias" editable="false" resizable="true" text="%Alias" />
                <TableColumn id="tcItemsCount" fx:id="tcItemsCount" editable="false" resizable="true" text="%# ArtÃ­culos" />
                <TableColumn id="tcAmount" fx:id="tcAmount" editable="false" resizable="true" text="%Importe" />
                <TableColumn id="tcTill" fx:id="tcTill" editable="false" resizable="true" text="%Caja" />
              </columns>
              <placeholder>
		        <Label text="%No se han encontrado cestas aparcadas."/>
		      </placeholder>
            </TableView>
          </children>
          <padding>
            <Insets fx:id="x1" />
          </padding>
        </HBox>
        <HBox alignment="CENTER_RIGHT" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0" styleClass="button-panel-box" VBox.vgrow="NEVER">
          <children>
            <Button fx:id="btAccept" mnemonicParsing="false" onAction="#acceptSelectedDocument" prefHeight="42.0" prefWidth="115.0" styleClass="main-btn" text="%Aceptar" />
            <Button fx:id="btCancel" mnemonicParsing="false" onAction="#closeCancel" prefHeight="42.0" prefWidth="115.0" styleClass="btn" text="%Cancelar" />
          </children>
          <padding>
            <Insets fx:id="x1" />
          </padding>
        </HBox>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@../../../styles/styles.css" />
  </stylesheets>
</AnchorPane>
