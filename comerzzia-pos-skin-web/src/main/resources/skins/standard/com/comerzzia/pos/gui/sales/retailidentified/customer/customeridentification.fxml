<AnchorPane id="AnchorPane" prefHeight="728.0" prefWidth="1024.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <VBox prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <HBox alignment="CENTER" minHeight="48.0">
          <children>
            <CzzHeader fx:id="header" />
          </children>
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
          <children>
            <VBox prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS">
              <children>
                <WebView fx:id="wvMain" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
              </children>
            </VBox>
            <VBox prefHeight="-1.0" prefWidth="350.0" spacing="10.0">
              <children>
                <HBox prefHeight="75.0" prefWidth="-1.0" />
                <HBox prefHeight="41.0" prefWidth="-1.0">
                  <children>
                    <TextField fx:id="tfIdentificationCode" onKeyReleased="#actionSearchKeyboard" prefHeight="41.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
                  </children>
                </HBox>
                <HBox minHeight="300.0" prefHeight="-1.0">
                  <children>
                    <NumericKeypad fx:id="numpad" />
                  </children>
                </HBox>
                <VBox fx:id="vbBottomButtonsPanel" prefHeight="130.0" VBox.vgrow="ALWAYS">
                  <VBox.margin>
                    <Insets bottom="15.0" />
                  </VBox.margin>
                </VBox>
              </children>
              <padding>
                <Insets left="15.0" right="15.0" />
              </padding>
            </VBox>
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@../../../styles/styles.css" />
  </stylesheets>
</AnchorPane>
