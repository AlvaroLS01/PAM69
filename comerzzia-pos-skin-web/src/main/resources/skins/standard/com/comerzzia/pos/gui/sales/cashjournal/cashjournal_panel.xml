<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ButtonsGroup>
    <ButtonsRow>
    	<Button>
			<text>Abrir cajón</text>
            <key>ABRIR CAJON</key>
            <type>OPERACION</type>
			<shortcut>Ctrl + O</shortcut>
			<visible>N</visible>
        </Button>
        <Button>
			<text>Apertura</text>
            <key>cashOpening</key>
            <type>METODO</type>
			<shortcut>F1</shortcut>
			<visible>S</visible>
        </Button>  
		<Button>
			<text>Cierre</text>
            <key>cashClose</key>
            <type>METODO</type>
			<shortcut>F2</shortcut>
			<visible>S</visible>
        </Button>
        <Button>
			<text>Imp. último cierre</text>
            <key>printLastCloseUp</key>
            <type>METODO</type>
			<shortcut>F3</shortcut>
			<visible>S</visible>
        </Button>
       	<Button>
			<text>Insertar apunte</text>
            <key>insertTransaction</key>
            <type>METODO</type>
			<shortcut>F4</shortcut>
			<visible>S</visible>
        </Button>  
       	<Button>
			<text>Recuento</text>
            <key>cashCountOpening</key>
            <type>METODO</type>
			<shortcut>F5</shortcut>
			<visible>S</visible>
        </Button> 
       	<Button>
			<text>Cajero</text>
            <key>changeCashier</key>
            <type>METODO</type>
			<shortcut>F6</shortcut>
			<visible>S</visible>
        </Button>
    </ButtonsRow>
</ButtonsGroup>