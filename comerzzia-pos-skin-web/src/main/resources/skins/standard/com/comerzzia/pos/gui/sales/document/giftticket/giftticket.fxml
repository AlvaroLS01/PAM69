<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-stylesheet ../../../styles/colors.css?>
<?scenebuilder-stylesheet ../../../styles/styles.css?>

<AnchorPane id="AnchorPane" prefHeight="607.0" prefWidth="811.0" styleClass="pantalla-detalle, mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <VBox prefHeight="200.0" prefWidth="100.0" styleClass="background-card-pane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <HBox minHeight="-1.0" prefHeight="-1.0" prefWidth="-1.0" styleClass="titulo-ventana">
          <children>
            <Label alignment="CENTER" minHeight="42.0" prefHeight="42.0" prefWidth="-1.0" text="%Ticket regalo" textAlignment="LEFT" HBox.hgrow="ALWAYS" />
          </children>
        </HBox>
        <VBox prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
          <children>
            <HBox prefHeight="-1.0" prefWidth="-1.0" styleClass="card" VBox.vgrow="ALWAYS">
              <children>
                <TableView fx:id="tbDocument" editable="false" prefHeight="-1.0" prefWidth="-1.0" tableMenuButtonVisible="false" HBox.hgrow="ALWAYS">
                  <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                  </columnResizePolicy>
                  <columns>
                    <TableColumn maxWidth="200.0" minWidth="80.0" prefWidth="80.0" text="%ARTÍCULO" fx:id="tcItemCode" />
                    <TableColumn maxWidth="5000.0" minWidth="100.0" prefWidth="340.0" text="%DESCRIPCIÓN" fx:id="tcItemDes" />
                    <TableColumn maxWidth="60.0" minWidth="20.0" prefWidth="60.0" text="%UND." fx:id="tcQuantity" />
                    <TableColumn maxWidth="70.0" minWidth="20.0" prefWidth="60.0" text="" fx:id="tcCombination1" />
                    <TableColumn maxWidth="70.0" minWidth="20.0" prefWidth="60.0" text="" fx:id="tcCombination2" />
                    <TableColumn maxWidth="60.0" minWidth="20.0" prefWidth="20.0" text="" fx:id="tcBtSelec" />
                  </columns>
                </TableView>
              </children>
            </HBox>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="53.0" prefWidth="811.0" styleClass="button-panel-box">
              <children>
                <Button fx:id="btAccept" mnemonicParsing="false" onAction="#actionAccept" prefHeight="42.0" prefWidth="115.0" styleClass="btAceptar" text="%Aceptar" />
                <Button fx:id="btCancel" mnemonicParsing="false" onAction="#closeCancel" prefHeight="52.0" prefWidth="115.0" styleClass="btCancelar" text="%Cancelar" />
              </children>
            </HBox>
          </children>
        </VBox>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@../../../styles/styles.css" />
  </stylesheets>
</AnchorPane>
