<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-stylesheet ../../../../styles/colors.css?>
<?scenebuilder-stylesheet ../../../../styles/styles.css?>

<AnchorPane id="AnchorPane" fx:id="panelContenido" prefHeight="768.0" prefWidth="1024.0" styleClass="mainFxmlClass, pantalla-detalle" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" >
  <children>
    <VBox prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <HBox prefHeight="50.0" prefWidth="-1.0" styleClass="titulo-ventana">
          <children>
            <Label alignment="CENTER" prefHeight="41.0" prefWidth="871.0" text="%Marcar venta artículos" />
          </children>
        </HBox>
        <HBox prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
          <children>
            <VBox prefHeight="-1.0" prefWidth="-1.0" styleClass="background-card-pane" HBox.hgrow="ALWAYS">
              <children>
                <HBox prefHeight="-1.0" prefWidth="-1.0" styleClass="card">
                  <children>
                    <AnchorPane fx:id="panelBotonera" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="NEVER" />
                  </children>
                </HBox>
                <HBox prefHeight="-1.0" prefWidth="-1.0" styleClass="card" VBox.vgrow="ALWAYS">
                  <children>
                    <TableView fx:id="tbArticulos" onMouseReleased="#aceptarArticuloDobleClick" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS">
                      <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                      </columnResizePolicy>
                      <columns>
                        <TableColumn maxWidth="150.0" minWidth="10.0" prefWidth="100.0" text="%Artículo" fx:id="tcArticulo" />
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="181.0" text="%Descripción" fx:id="tcDescripcion" />
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="74.0" text="%Talla" fx:id="tcDesglose1" />
                        <TableColumn prefWidth="75.0" text="%Color" fx:id="tcDesglose2" />
                        <TableColumn prefWidth="75.0" text="%Cantidad" fx:id="tcCantidad" />
                        <TableColumn prefWidth="75.0" text="%PVP" fx:id="tcPrecio" />
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="60.0" text="%Dto." fx:id="tcDescuento" />
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="60.0" text="%Importe" fx:id="tcImporte" />
                        <TableColumn maxWidth="60.0" prefWidth="75.0" text="" fx:id="tcBtSelec" />
                      </columns>
                    </TableView>
                  </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0">
                  <children>
                    <Button mnemonicParsing="false" onAction="#accionAceptar" prefWidth="120.0" styleClass="btAceptar" text="%Aceptar" />
                    <Button mnemonicParsing="false" onAction="#closeCancel" prefWidth="120.0" styleClass="btCancelar" text="%Cancelar" />
                  </children>
                  <padding>
                    <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                  </padding>
                </HBox>
              </children>
            </VBox>
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@../../../../styles/styles.css" />
  </stylesheets>
</AnchorPane>
